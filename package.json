{
  "name": "three-text2d",
  "version": "0.2.3-rc1",
  "description": "Render texture from canvas into THREE's Mesh or Sprite.",
  "author": "Endel Dreyer <endel.dreyer@gmail.com>",
  "keywords": [
    "three.js",
    "text",
    "canvas",
    "font"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/gamestdio/three-text2d.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "dist": "./node_modules/browserify/bin/cmd.js src/index.js -t babelify -s THREE_Text -o dist/three-text2d.js",
    "prepublish": "babel src -d lib"
  },
  "license": "MIT",
  "peerDependencies": {
    "three": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "three": "*"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
